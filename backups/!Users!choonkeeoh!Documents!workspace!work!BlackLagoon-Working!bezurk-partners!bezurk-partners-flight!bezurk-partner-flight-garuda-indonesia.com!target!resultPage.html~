<script type="text/javascript">$(document).ready(function() {
	//$("#boxNewMember").dialog({bgiframe:true, autoOpen:false, modal:true, width:500, stack:false});
	//$("#boxMemberLogin").dialog({bgiframe:true, autoOpen:false, modal:true, resizable:false});
	$('a[class*=btnLogin]').click(function() { $('#boxMemberLogin').dialog('open'); return false; });
	$("#boxMemberLogin").dialog({bgiframe:true, autoOpen:false, modal:true, resizable:false});
	$('a[class*=btnNewMember]').click(function() { $('#boxNewMember').dialog('open'); $("#boxMemberLogin").dialog('close'); $('#boxResetPin').dialog('close'); return false;});
	$("#boxNewMember").dialog({bgiframe:true, autoOpen:false, modal:true, width:500, stack:false});
	$("#birthdate").datepicker();
	$('#btnSubmitNewMember').click(function() {
		$.post(
			"https://ibb.garuda-indonesia.com/ibp/member/register",
			$('#frmNewMember').serialize(), 
			function(data) {
				if (data.error=='') {
					alert(data.msg);
					$("#boxNewMember").dialog('close'); window.location.reload();
				} else {
					alert(data.error+"\n\n"+data.msg);
				}
			}, "json");
	});
	$('#btnLogin').click(function() {
		$.post("https://ibb.garuda-indonesia.com/ibp/member/login", $('#frmMemberLogin').serialize(), function(msg) {alert(msg); $("#boxMemberLogin").dialog('close'); window.location.reload();});
	});
	$('a[class*=btnLogout]').click(function() {
		if (confirm('Are you sure to log out?')) {
			$.get("https://ibb.garuda-indonesia.com/ibp/member/logout", '', function(msg) {alert(msg); window.location.reload();});
		}
	});
	$('a[class*=btnFogetPin]').click(function() { $('#boxResetPin').dialog('open'); $('#boxNewMember').dialog('close'); $("#boxMemberLogin").dialog('close'); return false;});
	$("#boxResetPin").dialog({bgiframe:true, autoOpen:false, modal:true, width:500, stack:false});
	$('#btnSubmitResetPin').click(function() { $.post("https://ibb.garuda-indonesia.com/ibp/member/resetPin", $('#frmResetPin').serialize(), function(data) { alert(data.msg); $("#boxResetPin").dialog('close'); $('#frmResetPin')[0].reset(); }, "json"); });
});</script>
<script type="text/javascript">$(document).ready(function() {
	var Fares = {"NEE1MW":{"value":958500,"valueChd":862650,"valueChdSatu":958500,"valueChdDua":718875,"class":"N","bkgClass":"N","type":"NEE1MW","rule":"WB06","validity":"1M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":"12 NEE1MW   \/            1917000\/N\/   . 1M\/01JUN 31DEC\/WB06 R\n"},"KEE2MW":{"value":1053000,"valueChd":947700,"valueChdSatu":1053000,"valueChdDua":789750,"class":"K","bkgClass":"K","type":"KEE2MW","rule":"WB05","validity":"2M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":"10 KEE2MW   \/            2106000\/K\/   . 2M\/01JUN 31DEC\/WB05 R\n"},"LEE3MW":{"value":1138000,"valueChd":1024200,"valueChdSatu":1138000,"valueChdDua":853500,"class":"L","bkgClass":"L","type":"LEE3MW","rule":"WB04","validity":"3M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 8 LEE3MW   \/            2276000\/L\/   . 3M\/01JUN 31DEC\/WB04 R\n "},"MEE4MW":{"value":1267500,"valueChd":1140750,"valueChdSatu":1267500,"valueChdDua":950625,"class":"M","bkgClass":"M","type":"MEE4MW","rule":"WB03","validity":"4M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 6 MEE4MW   \/            2535000\/M\/   . 4M\/01JUN 31DEC\/WB03 R\n "},"YRTW":{"value":1326000,"valueChd":1193400,"valueChdSatu":1326000,"valueChdDua":994500,"class":"Y","bkgClass":"Y","type":"YRTW","rule":"WB02","validity":"6M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 4 YRTW     \/            2652000\/Y\/   . 6M\/01JUN 31DEC\/WB02 R\n "}};
			var Faresr = {"NEE1MW":{"value":958500,"valueChd":862650,"valueChdSatu":958500,"valueChdDua":718875,"class":"N","bkgClass":"N","type":"NEE1MW","rule":"WB06","validity":"1M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":"12 NEE1MW   \/            1917000\/N\/   . 1M\/01JUN 31DEC\/WB06 R\n"},"KEE2MW":{"value":1053000,"valueChd":947700,"valueChdSatu":1053000,"valueChdDua":789750,"class":"K","bkgClass":"K","type":"KEE2MW","rule":"WB05","validity":"2M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":"10 KEE2MW   \/            2106000\/K\/   . 2M\/01JUN 31DEC\/WB05 R\n"},"LEE3MW":{"value":1138000,"valueChd":1024200,"valueChdSatu":1138000,"valueChdDua":853500,"class":"L","bkgClass":"L","type":"LEE3MW","rule":"WB04","validity":"3M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 8 LEE3MW   \/            2276000\/L\/   . 3M\/01JUN 31DEC\/WB04 R\n "},"MEE4MW":{"value":1267500,"valueChd":1140750,"valueChdSatu":1267500,"valueChdDua":950625,"class":"M","bkgClass":"M","type":"MEE4MW","rule":"WB03","validity":"4M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 6 MEE4MW   \/            2535000\/M\/   . 4M\/01JUN 31DEC\/WB03 R\n "},"YRTW":{"value":1326000,"valueChd":1193400,"valueChdSatu":1326000,"valueChdDua":994500,"class":"Y","bkgClass":"Y","type":"YRTW","rule":"WB02","validity":"6M","effective":"01JUN 31DEC","effectiveStart":"2010-06-01","effectiveEnd":"2010-12-31","restrictDaysOutbnd":null,"restrictFlightsOutbnd":null,"restrictDaysInbnd":null,"restrictFlightsInbnd":null,"all":" 4 YRTW     \/            2652000\/Y\/   . 6M\/01JUN 31DEC\/WB02 R\n "}};
		var Taxes = {"yq":0,"iw":20000,"currency":"IDR","all":""};
	var adtPaxNum = 1;
	var chdPaxNum = 0;
	var bkgClassLevel = ['V','B','Q','N','K','L','M','Y','C'];
	$("td.tdcell").click(function() {
		if ($(this).children('.node').length<1) return false;
		$("td.tdcell.bgorange").removeClass("bgorange");
		$(this).addClass("bgorange");
		var node = $(this).children('.node').val();
		$('#outNode').val(node);
		calcSidebarFare();
	});
	$("td.tdcellret").click(function() {
		if ($(this).children('.node').length<1) return false;
		$("td.tdcellret.bgorange").removeClass("bgorange");
		$(this).addClass("bgorange");
		var node = $(this).children('.node').val();
		$('#retNode').val(node);
		calcSidebarFare();
	});
	calcSidebarFare = function() {
		var oNode = $('#outNode').val();
		if (oNode=='') return false;
		var oBkgClass = oNode.split('.')[1];
		var oFare = Fares[oBkgClass];
		if ($('#retNode').length>0) {
			var rNode = $('#retNode').val();
			if (rNode=='') return false;
			var rBkgClass = rNode.split('.')[1];
			var rFare = Faresr[rBkgClass];
			var _basicFareAdt = parseInt(oFare.value) + parseInt(rFare.value);
			var basicFareAdt = Math.ceil(_basicFareAdt/1000)*1000;
			var totalFareAdt = basicFareAdt * adtPaxNum;
			var effectiveBkgClass = (jQuery.inArray(oFare.bkgClass, bkgClassLevel)>jQuery.inArray(rFare.bkgClass, bkgClassLevel)) ? rFare.bkgClass : oFare.bkgClass;
			var _basicFareChd = (jQuery.inArray(effectiveBkgClass, ['C','Y','M','L'])>-1)?oFare.valueChdDua+rFare.valueChdDua:oFare.valueChdSatu+rFare.valueChdSatu;
			var basicFareChd = Math.ceil(_basicFareChd/1000)*1000;
			var totalFareChd = basicFareChd * chdPaxNum;
		} else {
			var _basicFareChd = (jQuery.inArray(oFare.bkgClass, ['C','Y','M','L'])>-1)?oFare.valueChdDua:oFare.valueChdSatu;
			var totalFareAdt = oFare.value * adtPaxNum;
			var totalFareChd = _basicFareChd * chdPaxNum;
		}
		var totalPaxFare = totalFareAdt + totalFareChd;
		var vat = totalPaxFare * 0.1;
		var totalYq = Taxes.yq * (adtPaxNum+chdPaxNum);
		var totalIw = Taxes.iw * (adtPaxNum+chdPaxNum);
		var totalFare = totalPaxFare + vat + totalYq + totalIw;
		$('#sidebarBaseFareAdt').text(nf(totalFareAdt));
		$('#sidebarBaseFareChd').text(nf(totalFareChd));
		$('#sidebarTax').text(nf(vat));
		if (totalYq>0) { $('#sidebarYq').text(nf(totalYq)); } else {$('#sidebarYqBlock').hide();}
		$('#sidebarIw').text(nf(totalIw));
		$('#sidebarTotal').text(nf(totalFare));
		$('#sidebarTotalBrekadown').text(nf(totalFare));
		$('#boxBreakdownFare').show();
	};
	$("#btnSubmit").click(function() {
		var submit = false;
		if ($('#outNode').val() != '') submit = true;
		if ($('#retNode').length>0 && $('#retNode').val() == '') submit = false;
		if (submit) $('#frmFlightPrice').submit(); else alert('Please select flight and fare.');
	});
});</script>
