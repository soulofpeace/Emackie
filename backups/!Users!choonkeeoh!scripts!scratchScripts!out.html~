<script>   var DayCssClassCache
   function SelectFlightCombo(d1,d2,f1,f2,o,oo,x,y){
		var t = document.getElementById(oo+'_FareshopperCombo')
       var d1v = document.getElementById(o + '_SelectedDateOutbound')
       var d2v = document.getElementById(o + '_SelectedDateInbound')
       var f1v = document.getElementById(o + '_FlightFareOutBound')
       var f2v = document.getElementById(o + '_FlightFareInBound')
       d1v.value = d1 
       d2v.value = d2 
       f1v.value = f1 
       f2v.value = f2 
       t.rows[x].cells[y].childNodes[0].className = 'SelectDayCssClass'
       DayCssClassCache = 'SelectDayCssClass'
       t.rows[XctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek].cells[YctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek].childNodes[0].className = YctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek
       XctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek = x
       YctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek = y
       CctrHome_ctrFareShopperWeekBase_ctrFareShopperWeek_ctrTikAeroFareshopperWeek =  t.rows[x].cells[y].childNodes[0].className
   }
   function FlightCombo_over(x,y,o){
		var t = document.getElementById(o+'_FareshopperCombo')
       t.rows[0].cells[y].className = 'DaysOutboundCssClass_over'
       t.rows[x].cells[0].className = 'DaysInboundCssClass_over'
       if(t.rows[x].cells[y].childNodes[0].tagName == 'DIV'){ 
           DayCssClassCache = t.rows[x].cells[y].childNodes[0].className
           t.rows[x].cells[y].childNodes[0].className = 'DayCssClass_over'
       }
   }
   function FlightCombo_out(x,y,o){
		var t = document.getElementById(o+'_FareshopperCombo')
       t.rows[0].cells[y].className = 'DaysOutboundCssClass'
       t.rows[x].cells[0].className = 'DaysInboundCssClass'
       if(t.rows[x].cells[y].childNodes[0].tagName == 'DIV'){ 
       t.rows[x].cells[y].childNodes[0].className = DayCssClassCache
       }
   }</script>


